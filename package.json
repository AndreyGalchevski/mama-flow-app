{
  "name": "mama-flow-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "doctor": "npx expo-doctor",
    "lint": "biome check --write",
    "lint:check": "biome check",
    "type-check": "tsc --noEmit",
    "watch-ts": "tsc --noEmit --watch",
    "prebuild": "npm run lint:check && npm run type-check",
    "build:android:development": "eas build -p android --profile development",
    "build:android:preview": "eas build -p android --profile preview",
    "build:android:production": "eas build -p android --profile production",
    "build:ios:simulator": "eas build --platform ios --profile ios-simulator",
    "build:ios:preview": "eas build -p ios --profile preview",
    "build:ios:production": "eas build -p ios --profile production",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "update:production": "eas update --branch production",
    "update:preview": "eas update --branch preview"
  },
  "dependencies": {
    "@bottom-tabs/react-navigation": "^0.9.2",
    "@react-navigation/native": "^7.1.6",
    "@sentry/react-native": "~6.14.0",
    "@shopify/flash-list": "^2.0.2",
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "date-fns": "^4.1.0",
    "expo": "~53.0.20",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.2",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.4",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "^2.2.3",
    "expo-store-review": "~8.1.5",
    "i18n-js": "^4.5.1",
    "papaparse": "^5.5.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-bottom-tabs": "^0.9.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-mmkv": "^3.3.0",
    "react-native-paper": "^5.14.5",
    "react-native-paper-dates": "^0.22.47",
    "react-native-paper-dropdown": "^2.3.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "zod": "^4.0.15",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "^1.9.4",
    "@types/d3-array": "^3.2.1",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.7",
    "@types/papaparse": "^5.3.16",
    "@types/react": "~19.0.10",
    "expo-dev-client": "~5.2.4",
    "typescript": "~5.8.3"
  },
  "private": true,
  "license": "MIT",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
