{
  "expo": {
    "name": "MamaFlow",
    "slug": "mama-flow",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mamaflow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "A comprehensive pumping log tracker for breastfeeding mothers. Track volume, duration, and schedule reminders to maintain your pumping routine.",
    "primaryColor": "#4F8A8B",
    "githubUrl": "https://github.com/AndreyGalchevski/mama-flow-app",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soyrage.mamaflow",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.soyrage.mamaflow",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "react-native-bottom-tabs",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-localization"
    ],
    "notification": {
      "icon": "./assets/images/ic_notification.png",
      "color": "#4F8A8B"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "38532903-8a8a-48c6-8716-bf73e9769679"
      },
      "privacyPolicyUrl": "https://www.privacypolicies.com/live/72f69876-661a-418e-be05-37d7df86dcf0"
    },
    "owner": "andreygalchevski"
  }
}
